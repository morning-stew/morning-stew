    "node-cron": "^4.2.1",
    "playwright": "^1.40.0",
    "tsx": "^4.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@solana/spl-token": "^0.4.14",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
